{
	"name": "thematic-ghost",
	"repo": "ksnyde/thematic-ghost",
	"version": "0.0.1",
	"ignore": [
		"**/.*",
		"node_modules",
		"components"
	],
	"dependencies": {
		"modernizr": "2.*",
		"jquery": "2.1.0",
		"bootstrap": "~3.2.*",
		"handlebars": "1.*",
		"font-awesome": "4.*",
		"moment": "~2.*"
	},
	"overrides": {
		"modernizr": {
			"main":"modernizr.js",
			"dependencies":{}
		},
		"jquery": {
			"main": "jquery.js"
		},
		"bootstrap": {
			"main": [
				"dist/js/bootstrap.js",
				"dist/css/bootstrap.css",
				"dist/fonts/glyphicons-halflings-regular.woff",
				"dist/fonts/glyphicons-halflings-regular.ttf",
				"dist/fonts/glyphicons-halflings-regular.svg"	
			],
			"dependencies": {
				"jquery": ">= 1.9.0"
			}
		},
		"font-awesome":{
			"main": "css/font-awesome.css"
		},
		"moment": {
			"main": "moment.js"
		},
		"resolutions": {
			"bootstrap": "~3.0.0"
		}
	}
}
